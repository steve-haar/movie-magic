{
  "name": "movie-magic",
  "description": "An example of using Code Shaper to generate a React app",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "configs/*",
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "graph": "turbo run build --graph",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rimraf node_modules",
    "prepare": "husky install",
    "format": "prettier --write \"**/README.md\" \"**/src/**/*.{js,jsx,ts,tsx,json}\"",
    "storybook": "cd storybook && npm run storybook",
    "build-storybook": "cd storybook && npm run build-storybook",
    "cypress": "cd apps/movie-magic-react && npm run cypress"
  },
  "devDependencies": {
    "@code-shaper/express": "latest",
    "@code-shaper/nextjs": "latest",
    "@code-shaper/plugin": "latest",
    "@code-shaper/react": "latest",
    "@code-shaper/remix": "latest",
    "@code-shaper/shaper-utils": "latest",
    "@code-shaper/typescript": "latest",
    "@movie-magic/react-patterns": "*",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "turbo": "latest"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.15.0"
}
